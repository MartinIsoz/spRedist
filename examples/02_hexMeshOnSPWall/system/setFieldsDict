FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue sootVolFrac 0
);

regions
(
    boxToCell
    {
        box (-10 -10 -10) (10 0.125 10);
        fieldValues
        (
            volScalarFieldValue sootVolFrac 1
        );
    }
);


