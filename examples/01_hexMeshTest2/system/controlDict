FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     spRedistApp;

//~ startFrom       startTime;
startFrom       latestTime;

startTime       0;

stopAt          endTime;
//~ stopAt          writeNow;

//~ endTime         200;
endTime         128;

deltaT          1;
writeControl    timeStep;
writeFrequency  1;

purgeWrite      0;

writeFormat     ascii;

writePrecision  8;

writeCompression uncompressed;

timeFormat      general;

timePrecision   8;

runTimeModifiable yes;



